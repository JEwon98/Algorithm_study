    "#boj 4963\n",
    "#월요일도 바쁘니까 미리하자"
    "dx = [1, -1, 0, 0, 1, -1, 1, -1]
     dy = [0, 0, -1, 1, -1, -1, 1, 1]
     def bfs(i, j):
      s[i][j] = 0
      queue = [[i, j]]
     while queue:
         a, b = queue[0][0], queue[0][1]
         del queue[0]
         for k in range(8):
             x = a + dx[k]
             y = b + dy[k]
             if 0 <= x < h and 0 <= y < w and s[x][y] == 1:
                 s[x][y] = 0
                 queue.append([x, y])
    while True:
        w, h = map(int, input().split())
        if w == 0 and h == 0:
            break
        s = []
        cnt = 0
        for i in range(h):
            s.append(list(map(int, input().split())))
        for i in range(h):
            for j in range(w):
                if s[i][j] == 1:
                    bfs(i, j)
                    cnt += 1
        print(cnt)"
